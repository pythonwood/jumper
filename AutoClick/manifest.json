{
 "name":"监控页面自动切换插件",
 "manifest_version": 2,
 "version":"1.0",
 "description":"内部使用",
 "background": {
 "scripts": ["background.js"]
 },
 "content_scripts":[{
 "matches" : [ "http://*/*" ],
 "js": [ "jquery-1.8.3.min.js","jquery.cookie.js","popup.js" ],
 "run_at" : "document_end"
 }
 ],
 "browser_action":{
 "default_icon":"icon.png",
 "default_popup":"popup.html"
 }
}